{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.palewire/datawrapper-mcp",
  "title": "Datawrapper",
  "description": "A Model Context Protocol (MCP) server for creating Datawrapper charts using AI assistants",
  "version": "0.0.18",
  "repository": {
    "url": "https://github.com/palewire/datawrapper-mcp",
    "source": "github"
  },
  "packages": [
    {
      "registryType": "pypi",
      "registryBaseUrl": "https://pypi.org",
      "identifier": "datawrapper-mcp",
      "version": "0.0.16",
      "runtimeHint": "uvx",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "DATAWRAPPER_ACCESS_TOKEN",
          "description": "Datawrapper API Access Token",
          "isRequired": true,
          "isSecret": true
        }
      ]
    }
  ]
}
